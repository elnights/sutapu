<%- partial('nav_top')%>
<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="span3">
        <%- partial('nav_side')%>
      </div>
      <div class="span9">
        <h1>Topics</h1>
        <table class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>Id</th>
              <th>Name</th>
              <th>User</th>
              <th>Updated</th>
              <th>Created</th>
            </tr>
          </thead>
          <tbody>
            <% _.each(items, function (topic) { %>
              <tr>
                <td><%= topic.id %></td>
                <td><%= topic.name %></td>
                <td><%= topic.user %></td>
                <td class='for-moment'><%= topic.createdAt %></td>
                <td class='for-moment'><%= topic.updatedAt %></td>
              </tr>
            <% }) %>  
          </tbody>
        </table>
        <%- partial('pagination')%>
        <button class='btn-small pull-right btn btn-primary'>New Topic</button>
      </div>
    </div>
  </div>
</div>